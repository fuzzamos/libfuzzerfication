<?xml version="1.0"?>
<!-- 

	This scripts dowloads xml.xsd to support local validation.
	
	ChangeLog:
	
	2006-05-19 Arthur Ryman (ryman@ca.ibm.com)
	- created

-->

<project name="schemas" default="default">

	<description>
            Gets xml.xsd
    </description>

	<target name="default" description="--> Get xml.xsd">

		<get dest="xml.xsd" src="http://www.w3.org/2001/xml.xsd" />

	</target>

</project>

