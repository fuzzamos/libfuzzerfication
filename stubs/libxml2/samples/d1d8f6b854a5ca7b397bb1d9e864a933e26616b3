<?xml version="1.0" encoding="UTF-8"?>
<wsp:Policy xmlns:wsp="http://www.w3.org/ns/ws-policy" xmlns:x="http://example.org/mypolicy">
    <wsp:ExactlyOne>
        <wsp:TwoOutOfThreeAintBad>
            <x:A/>
            <x:B/>
            <x:C/>
        </wsp:TwoOutOfThreeAintBad>
        <wsp:All>
            <x:D/>
            <x:E/>
        </wsp:All>
    </wsp:ExactlyOne>
</wsp:Policy>
