<?xml version="1.0" ?>

<toolkit xml:id="xfire_java_125_xmlbeans"
    name="XFire" 
    mapping="Java"
    version="1.2.5"
    binding="XMLBeans"
    link="http://xfire.codehaus.org"
    content_type="text/xml; charset=utf-8"
    endpoint="http://localhost:8080/databinding/services/Service">

    <!-- examples known to cause problems consuming the WSDL -->
    <exclude>AttributeElementNameClash</exclude>
    <exclude>NoTargetNamespace</exclude>

    <exclude>ElementReferenceUnqualified</exclude>
    <exclude>ElementTypeReferenceUnqualified</exclude>
    <exclude>AttributeReferenceUnqualified</exclude>
    <exclude>AttributeTypeReferenceUnqualified</exclude>
    <exclude>GlobalAttributeUnqualifiedType</exclude>
    <exclude>GlobalElementUnqualifiedType</exclude>
    <exclude>UnqualifiedLocalElements</exclude>
    <exclude>QualifiedLocalAttributes</exclude>
    <exclude>ImportSchema</exclude>
    <exclude>ImportSchemaNamespace</exclude>
    <exclude>ImportTypesNamespace</exclude>
    <exclude>ImportNamespace</exclude>

    <!-- examples which are broken -->
    <exclude>TargetNamespace</exclude>
    <exclude>IncludeRelative</exclude>
    <exclude>Include</exclude>
    <exclude>ChameleonInclude</exclude>
    <exclude>QualifiedLocalElements</exclude>
    <exclude>UnqualifiedLocalElements</exclude>
    <exclude>UnqualifiedLocalAttributes</exclude>
    <exclude>SchemaVersion</exclude>
    <exclude>FinalDefault</exclude>
    <exclude>BlockDefault</exclude>
    <exclude>SOAPEncodedArray</exclude>

    <exclude>GlobalElementFinal</exclude>
    <exclude>ElementFinal</exclude>

</toolkit>
