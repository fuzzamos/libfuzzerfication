<?xml version="1.0" encoding="UTF-8"?>

<sumoshader name="red">


	<shader type="fragment">

		<functions>
		uniform sampler2DRect map_diffuse;
		</functions>

		<main>
			color = color*texture2DRect(map_diffuse, vec2(gl_TexCoord[0].s,gl_TexCoord[0].t)); 
		</main>

	</shader>

</sumoshader>