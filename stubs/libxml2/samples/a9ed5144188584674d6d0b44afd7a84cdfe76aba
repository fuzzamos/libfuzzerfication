<?xml version="1.0"?>
<!-- 
    SOAP over Java Message Service Test Message Exchange Patterns
-->
<message-exchanges xmlns="http://www.w3.org/2008/09/soapjms/test-cases/message-exchange-patterns">

  <description>Message Exchange Patterns (MEPs) are an enumerated list of message exchanges which may be used
    in conjunction with the test case definitions to determine the coverage of  SOAP over Java Message Service 1.0 test cases.</description>

  <message-exchange xml:id="SOAP11-JMS-In-Only" addr-binding="soap11" addr-mep="soapjms-oneway">
    <messages>
      <message from="A" to="B" content="a SOAP 1.1 request message"/>
    </messages>
  </message-exchange>

  <message-exchange xml:id="SOAP12-JMS-In-Only" addr-binding="soap12" addr-mep="soapjms-oneway">
    <messages>
      <message from="A" to="B" content="a SOAP 1.2 request message"/>
    </messages>
  </message-exchange>

  <message-exchange xml:id="SOAP11-JMS-In-Out" addr-binding="soap11" addr-mep="soapjms-twoway">
    <messages>
      <message from="A" to="B" content="a SOAP 1.1 request message"/>
      <message from="B" to="A" content="a SOAP 1.1 response message"/>
    </messages>
  </message-exchange>

  <message-exchange xml:id="SOAP12-JMS-In-Out" addr-binding="soap12" addr-mep="soapjms-twoway">
    <messages>
      <message from="A" to="B" content="a SOAP 1.2 request message"/>
      <message from="B" to="A" content="a SOAP 1.2 response message"/>
    </messages>
  </message-exchange>

  <message-exchange xml:id="SOAP11-JMS-In-Out-Fault" addr-binding="soap11" addr-mep="soapjms-twoway-fault">
    <messages>
      <message from="A" to="B" content="a SOAP 1.1 request message"/>
      <message from="B" to="A" content="a SOAP 1.1 fault"/>
    </messages>
  </message-exchange>

  <message-exchange xml:id="SOAP12-JMS-In-Out-Fault" addr-binding="soap12" addr-mep="soapjms-twoway-fault">
    <messages>
      <message from="A" to="B" content="a SOAP 1.2 request message"/>
      <message from="B" to="A" content="a SOAP 1.2 fault"/>
    </messages>
  </message-exchange>

</message-exchanges>
