	<!ATTLIST svg
		xmlns:m CDATA #IMPLIED>

	<!ENTITY % defsExt "| m:canton">

	<!ELEMENT m:canton (m:name*, m:statistics, m:geometricInfo)>
	<!ATTLIST m:canton
		id ID #REQUIRED
		bndtype (village | county) #REQUIRED>

	<!ELEMENT m:name EMPTY>
	<!ATTLIST m:name
		lang (chinese | tonyonPinyin) " " " " "chineseÊ´"
		desc CDATA #REQUIRED>

	<!ELEMENT m:statistics (m:population)><!ELEMENT m:statistics (m:population)>

	<!ELEMENT m:population EMPTY>
	<!ATTLIST m:population
		count CDATA #REQUIRED>

	<!ELEMENT m:geometricInfo EMPTY>
	<!ATTLIST m:geometricInfo
		area CDATA #IMPLIED
		circumference CDATA #IMPLIED>

	<!ATTLIST polygon
		xlink:type (simple) #FIXED "simple"
		xlink:title CDATA #IMPLIED
		xlink:actuate (onRequest) #FIXED "onRequest"
		xlink:href CDATA #IMPLIED>
]>

