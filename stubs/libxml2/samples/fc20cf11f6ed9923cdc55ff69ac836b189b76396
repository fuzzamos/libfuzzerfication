 <!ENTITY long "long">
 <!ENTITY int "int">
 <!ENTITY short "short">
 <!ENTITY byte "byte">
 <!ENTITY unsigned-long "unsigned-long">
 <!ENTITY unsigned-int "unsigned-int">
 <!ENTITY unsigned-short "unsigned-short">
 <!ENTITY unsigned-byte "unsigned-byte">
