<?xml version="1.0"?>
<its:documentRules xmlns:its="http://www.w3.org/2005/11/its"
 xmlns="http://docbook.org/ns/docbook">

 <its:documentRule its:translateSelector="//firstname" its:translate="no"/>
 <its:documentRule its:translateSelector="//surname" its:translate="no"/>
 <its:documentRule its:translateSelector="//email" its:translate="no"/>
 <its:documentRule its:translateSelector="//funcdef" its:translate="no"/>
 <its:documentRule its:translateSelector="//equation/alt" its:translate="no"/>
 <its:documentRule its:translateSelector="//InvPartNumber" its:translate="no"/>
 <its:documentRule its:translateSelector="//ISSN" its:translate="no"/>
 <its:documentRule its:translateSelector="//IssueNum" its:translate="no"/>
 <its:documentRule its:translateSelector="//ProductNumber" its:translate="no"/>
 <its:documentRule its:translateSelector="//PubsNumber" its:translate="no"/>
 <its:documentRule its:translateSelector="//VolumeNum" its:translate="no"/>

 <its:documentRule its:locInfoSelector="//firstname" its:locInfo="Just transliterate the name." its:locInfoType="alert"/>

 <its:documentRule its:termSelector="//term" its:term="yes"/>
 
 <its:documentRule its:dirSelector="//*[@dir='ltr']" its:dir="ltr"/>
 <its:documentRule its:dirSelector="//*[@dir='rtl']" its:dir="rtl"/>
 <its:documentRule its:dirSelector="//*[@dir='lro']" its:dir="lro"/>
 <its:documentRule its:dirSelector="//*[@dir='rlo']" its:dir="rlo"/>

</its:documentRules>