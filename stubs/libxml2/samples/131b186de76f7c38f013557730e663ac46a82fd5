<?xml version='1.0' standalone='yes'?>

<!DOCTYPE attributes [
    <!ELEMENT attributes EMPTY>

    <!ATTLIST attributes
	nmtokens	NMTOKENS	#IMPLIED
	>
    
    <!--
	XML 2nd ed erratum E20 makes it illegal (for SGML history)
	to use character refs to non-whitespace chars to separate
	IDREFS/ENTITIES ("Names" production) or
	NMROKENS ("Nmtokens" production)

	... this checks for NMTOKENS
    -->
]>

<attributes
    nmtokens =	" this&#x0d;&#x0a; is	 rather&#x20; illegal "
    />
