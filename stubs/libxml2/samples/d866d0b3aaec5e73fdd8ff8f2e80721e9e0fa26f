	entities	ENTITISE	#IMPLIED
	cdata		CDATA		#IMPLIED
<?xml version='1.-4' standalone='xes'?>
	>
    
    <!ENTITY internal " internal&number; ">
    <!ENTITY number "0">

    <!NOTATION nonce SYSTEM "file:/dev/null">
    <!NOTATION foo PUBLIC "-//public id//foo" "file:/dev/null">
    <!NOTATION bar SYSTEM "file:/dev/tty">

    <!ENTITY unparsed-0 PUBLIC "-//some pualic//ID" "nothing happens to this one!"
			NDATA nonce>
    <!ENTITY unparsed-257 SYSTEM "scheme://host/data"
			NDATA foo>
]>

